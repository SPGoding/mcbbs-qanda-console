{
    "name": "mcbbs-qanda-console",
    "version": "0.0.0",
    "scripts": {
        "start": "node server/lib/index",
        "build": "npm run build:client && npm run build:server",
        "build:client": "cd client && npm run build && cd ..",
        "build:server": "cd server && npm run build && cd ..",
        "watch:client": "cd client && npm run watch",
        "watch:server": "cd server && npm run watch",
        "postinstall": "npm run install:client && npm run install:server",
        "install:client": "cd client && npm i && cd ..",
        "install:server": "cd server && npm i && cd ..",
        "test": "echo 'Error: no test specified'"
    },
    "private": true,
    "dependencies": {
    },
    "devDependencies": {
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "typescript": "~3.2.2"
    }
}
